<%= form_for @pstudent do |f| %>
  <%= f.label Name : (First,Last) %>:
  <%= f.text_field :first_name %>, 
  <%= f.text_field :last_name %><br />

  <%= f.label Age : %>:
  <%= f.number_field :age %>
  <%= f.label Sex  M/F : %>:
  <%= f.text_field :sex %>

  <%= f.label Date of Birth : %>:
  <%= f.date :birthday %>

  <%= f.label Diagonsis : %>:
  <%= f.date :diagnosis %>

  <%= f.submit %>
<% end %>